                <!-- Left Column -->
                <div class="w3-quarter">

                    <div class="w3-white w3-text-grey w3-card-4">
                        <div class="w3-display-container">
                            <img src="https://www.w3schools.com/w3images/avatar_hat.jpg" style="width: 100%" alt="Avatar">
