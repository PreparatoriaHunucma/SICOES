<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="mstrpgAdmin.master.cs" Inherits="SICOES2018.MasterPages.mstrpgAdmin" %>

<!DOCTYPE html>

<html>
<head runat="server">
