   <script>
       function Directivo(id) {
           var x = document.getElementById(id);
           if (x.className.indexOf("w3-show") == -1) {
               x.className += " w3-show";
               x.previousElementSibling.className =
                   x.previousElementSibling.className.replace("w3-amber", "w3-blue");
           } else {
               x.className = x.className.replace(" w3-show", "");
               x.previousElementSibling.className =
                   x.previousElementSibling.className.replace("w3-blue", "w3-amber");
           }
       }

